{
  "manifest_version": 2,
  "name": "Insta controls",
  "version": "1.0",
  "description": "Full Instagram video management added",
  "icons": {
    "128": "img/128x128.png"
  },
  "browser_action": {
    "default_icon": {
      "128": "img/128x128.png"
    },
    "default_title": "Control your Instagram videos"
  },
  "content_scripts": [
    {
      "matches": ["*://*.instagram.com/*"],
      "js": ["content.js"]
    }
  ]
}
